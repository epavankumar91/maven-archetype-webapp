pipeline {
  agent { label any }
  environment {
    VER = VersionNumber([versionNumberString : '${BUILD_YEAR}.${BUILD_MONTH}.${BUILD_DAY}.${BUILDS_ALL_TIME}', projectStartDate : '2018-11-25']);
  }
  stages { 
        stage('Cloning our Git') { 
            steps { 
                git 'https://github.com/rajkumar2289/maven-archetype-webapp.git' 
        }
        stage('ls Git') { 
            steps { 
                sh 'ls' 
        }
  } 
}
