pipeline {
  agent { label any }
  tools {
  git 'git'
  jdk 'jdk1.8.0'
  maven 'maven3.8.2'
}
 environment {
    VER = VersionNumber([versionNumberString : '${BUILD_YEAR}.${BUILD_MONTH}.${BUILD_DAY}.${BUILDS_NUMBER}', projectStartDate : '2021-08-22']);
  }
  stages { 
        stage('Cloning our Git') { 
            steps { 
                git 'https://github.com/rajkumar2289/maven-archetype-webapp.git' 
                }
         }
        stage('ls Git') { 
            steps { 
                sh 'ls ' 
                }
        }
        stage('Build Maven') { 
            steps { 
                sh 'clean package'
                }
        }
  } 
}
